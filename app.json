{
  "expo": {
    "name": "Lucid Journal",
    "slug": "lucid-journal",
    "scheme": "lucidjournal",
    "version": "1.3.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.jamesfeura.lucidjournal",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library to save dreams."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jamesfeura.lucidjournal",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ca62a3da-e797-4f07-aec8-f5f27ea698c5"
      }
    },
    "owner": "jamesfeura",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ca62a3da-e797-4f07-aec8-f5f27ea698c5"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    }
  }
}
